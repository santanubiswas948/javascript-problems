/**
Bind method polyfill
**/

Function.prototype.myBind = function (thisArg, ...argArray) {
  const fn = this;

  return function(){
    
    return fn.apply(thisArg, [...argArray, ...arguments]);

  }
};


/**

APPROACH 2:

**/

Function.prototype.myBind = function (thisArg, ...argArray) {
  const fn = this;

  return function(){

    thisArg.fn = fn;
    const args =  [...argArray, ...arguments];
    return thisArg.fn(...args);

  }
};
