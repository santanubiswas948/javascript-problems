/**
Polyfill for promise.all method
**/
Promise.prototype.mockAll = (arr) => {
  const ans = [];
  let count = 0;
  return new Promise((resolve, reject) => {
      if (arr.length === 0) resolve(arr);
      for(let i = 0; i<arr.length; i++) {
        Promise.resolve(arr[i]).then((res) => {
          ans[i] = res;
          if (++count === arr.length) {
            resolve(ans);
          }
        }).catch((err) => {
          reject(err);
        });
      }
  });
}
